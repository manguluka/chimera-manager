{
  "name": "base-project",
  "version": "1.0.0",
  "description": "> Events calendar for Cobot events",
  "main": "webpack.config.js",
  "scripts": {
    "build-assets": "webpack --colors",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --fix server frontend",
    "start": "node server",
    "test": "npm run test-server & npm run test-frontend",
    "test-frontend": "NODE_ENV=test karma start",
    "test-server": "NODE_ENV=test mocha 'server/**/*.test.js'",
    "watch": "parallelshell 'npm run watch-test' 'npm run watch-assets' 'npm run watch-server'",
    "watch-assets": "npm run build-assets -- --watch",
    "watch-server": "nodemon --exec npm start",
    "watch-test": "npm run watch-test-server npm run watch-test-frontend",
    "watch-test-server": "npm run test-server -- -w",
    "watch-test-frontend": "npm run test-frontend -- --auto-watch --no-single-run"
  },
  "author": "Dana Woodman <dana@danawoodman.com> (http://danawoodman.com/)",
  "license": "MIT",
